{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/index.js"
      }
    ],
    "build": {
      "env": {
        "TS_NODE_PROJECT": "tsconfig.json"
      },
      "commands": [
        "npm run build"
      ]
    }
  }